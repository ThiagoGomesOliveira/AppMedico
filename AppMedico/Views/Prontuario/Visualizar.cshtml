@model AppMedico.Models.x

@{
    ViewBag.Title = "Visualizar";
}
<h2 style="text-align:center ">Visualizar</h2>

<p>
    @Html.ActionLink("Voltar para Lista", "Index", null, new { @class = "btn btn-warning" })
</p>
<div class="panel-success">
        <div class="panel-heading">
            Registro do Prontuário
        </div>
            <div class="bg-info" id="informacao">
                <div class="form-control">
                    <span class="paragrafo">Codigo do Prontuario:@Model.IDProntuario</span> <br>
                </div>
                <div class="form-control">
                    <span class="paragrafo">Nome do Paciente:@Model.Paciente.Nome</span><br />
                </div>
                <div class="form-control">
                    <span class="paragrafo">Nome do Observação:@Model.Observacao</span>  <br />
                </div>
                <div class="form-control">
                    <span class="paragrafo">Tratamento:@Model.TratamentoPaciente</span> <br />
                </div>
                <div class="form-control">
                    <span class="paragrafo">Evolução do Paciente:@Model.EvolucaoPaciente</span> <br />
                </div>
                <div class="form-control">
                    <span class="paragrafo">Diagnostico :@Model.Diagnostico</span> <br />
                </div>
        </div>
</div>



